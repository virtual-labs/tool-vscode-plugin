{
  "name": "virtual-labs-experiment-generator",
  "displayName": "Virtual Labs Experiment Generator",
  "description": "A VS Code Plugin to handle all the common tasks invovled in virtual labs experiment development.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:virtual-labs-experiment-generator.virtualLabs"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "virtual-labs-experiment-generator.virtualLabs",
      "title": "Virtual Labs"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
